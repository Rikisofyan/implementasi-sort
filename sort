#include<iostream>
using namespace std;
 
int main(){
	cout<<"++++++++PENGURUTAN ANGKA +++++++++"<<endl<<endl;
    int a[50],n,i,j,srt;
    cout<<"masukan jumlah angka =  ";
    cin>>n;
    cout<<"\n\n";
    cout<<"masukan sembarang angka angka = "<<endl;
    
 for(i=0;i<n;++i)
        cin>>a[i];
        
    for(i=1;i<n;++i)
    {
        for(j=0;j<(n-i);++j)
            if(a[j]>a[j+1])
            {
                srt=a[j];
                a[j]=a[j+1];
                a[j+1]=srt;
            }
    }
    cout<<"urutan angka nya adalah = ";
    for(i=0;i<n;++i)
        cout<<" "<<a[i];
        
    return 0;
    
}
