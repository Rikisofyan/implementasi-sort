#include<iostream>
using namespace std;
 
int main(){
 for(i=0;i<n;++i)
        cin>>a[i];
        
    for(i=1;i<n;++i)
    {
        for(j=0;j<(n-i);++j)
            if(a[j]>a[j+1])
            {
                srt=a[j];
                a[j]=a[j+1];
                a[j+1]=srt;
            }
    }
    
}
